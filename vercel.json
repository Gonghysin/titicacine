{
    "version": 2,
    "builds": [
        {
            "src": "src/api.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "10mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api.py"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 