{
    "version": 2,
    "builds": [
        {
            "src": "src/api.py",
            "use": "@vercel/python@3.1.31",
            "config": {
                "runtime": "python3.9",
                "installCommand": "pip install -r requirements.txt",
                "pythonCommand": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9",
        "PYTHONPATH": "."
    }
} 