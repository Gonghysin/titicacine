{
    "version": 2,
    "builds": [
        {
            "src": "src/api.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/api.py"
        }
    ]
} 